{
    "name": "bellows-app/cli",
    "description": "An intelligent command line utility that sits on top of Laravel Forge to get your site up and running with all of your third party integrations configured at the speed of light.",
    "keywords": [
        "laravel",
        "console",
        "cli",
        "forge",
        "tools",
        "productivity"
    ],
    "homepage": "https://bellows.dev",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Joe Tannenbaum",
            "email": "joe@joe.codes"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/once": "^3.1"
    },
    "require-dev": {
        "composer/semver": "^3.3",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/http": "^10.0",
        "illuminate/log": "^10.0",
        "illuminate/validation": "^10.0",
        "illuminate/view": "^10.0",
        "laravel-zero/framework": "^10.0",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/larastan": "^2.0",
        "nunomaduro/termwind": "^1.14",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-faker": "^2.0",
        "phpstan/phpstan": "^1.10",
        "spatie/fork": "^1.1",
        "spatie/laravel-data": "^3.0",
        "spatie/laravel-ray": "^1.32",
        "spatie/php-structure-discoverer": "^1.1",
        "tightenco/duster": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Bellows\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "builds/bellows"
    ]
}
